<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <div class="card">
          <div class="card-header bg-white">
            <h3 class="card-title"><strong>Area Served</strong></h3>
          </div>
          <div class="card-body">

            <fieldset style="margin-bottom: 2rem">
              <legend></legend>
              <div id="partner_counties"  class="partner-county-fields">
                <%= f.simple_fields_for :partner_counties do |partner_county| %>
                  <%= render 'partner_counties/partner_county_fields', f: partner_county %>
                <% end %>
              </div>
              <div class="row links justify-content-end">
                <%= add_partner_county_button f, "#partner_counties" %>
                </div>
            </fieldset>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
